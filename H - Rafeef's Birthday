#include <iostream>
 
using namespace std;
 
int main()
{
   long long  n ;
   cin >> n ;
   if ( n >= 2000 && n <= 9000) {
 
   if ( n % 100 != 0 && n% 4 == 0 )
    cout << " YES " << endl ;
 
    else if ( n % 4 == 0&& n % 400==0)
        cout << " YES " << endl ;
 
        else
            cout << " NO " << endl ;
        }
       else
        cout << " NO " << endl ;
    return 0;
}
