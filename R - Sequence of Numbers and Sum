#include <iostream>
 
using namespace std;
 
int main()
{
    int N=1,M=1,sum=0;
 
 
 while(N > 0 && M > 0){
      cin >> N >> M;
      if(N <= 0 || M <= 0){
        break;}
 
      else{
        if(N>M){
        for(int i=M ; i<=N ; i++){
            cout << i << " " ;
                sum += i;
                }
        cout << "sum =" << sum << endl;}
 
      else{
        for(int i=N ; i<=M ; i++){
 
            cout << i << " " ;
                sum += i;}
 
 
          cout << "sum =" << sum << endl;}
          sum=0 ;
 }
        }
 
 
    return 0;
    }
