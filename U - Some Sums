#include <iostream>
 
using namespace std;
 
int main()
{
   int N,A,B,sum=0,sum1=0,x;
   cin >> N >> A >> B;
 
 for(int i=1 ; i<=N ; i++){
        x=i;
    while (x != 0)
    {
        sum = sum + x % 10;
        x = x / 10;
 
        }
 
 
     if(sum>=A&&sum<=B)
        sum1+=i;
        sum=0;
       }
 
     cout << sum1 ;
 
    return 0;
}
