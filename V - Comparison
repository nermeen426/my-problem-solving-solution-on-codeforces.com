#include <iostream>
using namespace std;
 
int main() {
int A , B ;
char comp ;
cin >> A >> comp >> B ;
if(comp == '>'){
  if(A > B)
  cout << "Right" << endl ;
  else 
  cout << "Wrong" << endl ;
    
}
else if(comp == '<'){
    if(A < B)
    cout << "Right" << endl ;
    else
    cout << "Wrong" << endl ;
}
else{
    if(A == B)
    cout << "Right" << endl ;
    else 
    cout << "Wrong" << endl ;
}
 
    return 0;
}
